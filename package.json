{
  "name": "input-validator-nodejs",
  "version": "1.0.1",
  "description": "validation library for nodejs, inspired by laravel.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src",
    "lintf": "eslint ./src --fix",
    "test": "mocha --recursive",
    "cover": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --recursive -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "engines": {
    "node": ">=7.6"
  },
  "keywords": [
    "validation",
    "validator",
    "data validation",
    "input validation",
    "input validator",
    "form validation",
    "form validator",
    "node validator",
    "laravel",
    "validator for js",
    "koa",
    "express",
    "file validation",
    "image",
    "mimes"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thachsinh/input-validator-nodejs.git"
  },
  "author": "thachsinh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thachsinh/input-validator-nodejs/issues"
  },
  "homepage": "https://github.com/thachsinh/input-validator-nodejs#readme",
  "dependencies": {
    "file-type": "^10.11.0",
    "image-size": "^0.7.4",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "read-chunk": "^3.2.0",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/moment": "^2.13.0",
    "@types/validator": "^10.11.1",
    "coveralls": "^3.0.5",
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "ncp": "^2.0.0",
    "should": "^13.2.3"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "@thachsinh:registry": "https://npm.pkg.github.com"
  }
}
